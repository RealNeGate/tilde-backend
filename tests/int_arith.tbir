# tests out integer arithmatic in all scalar types
def T [i32]

fn [T] add() {
	a = [T] PARAM
	b = [T] PARAM
	c = [T] a + b
	ret c
}

fn [T] sub() {
	a = [T] PARAM
	b = [T] PARAM
	c = [T] a - b
	ret c
}

fn [T] rsub() {
	a = [T] PARAM
	b = [T] PARAM
	c = [T] b - a
	ret c
}

fun mul() {
	a = [T] PARAM
	b = [T] PARAM
	c = [T] a * b
	ret c
}

fn [T] div() {
	a = [T] PARAM
	b = [T] PARAM
	c = a / b
	ret c
}

fn [T] div2() {
	a = [T] PARAM
	b = [T] 2
	c = [T] a / b
	ret c
}

fn [T] mul2() {
	a = [T] PARAM
	b = [T] 2	
	c = [T] a * b
	ret c
}
